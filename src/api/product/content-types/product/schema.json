{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "visible": {
      "type": "boolean"
    },
    "gender": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "boy",
        "girl"
      ]
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "New",
        "Outlet",
        "Favourite"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "outfit",
        "pelele",
        "dress"
      ]
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategory.subcategory",
      "mappedBy": "product"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "product"
    }
  }
}
