{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "string"
    },
    "subcategorias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategoria.subcategoria",
      "mappedBy": "categoria"
    },
    "producto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::producto.producto",
      "inversedBy": "categoria"
    }
  }
}
